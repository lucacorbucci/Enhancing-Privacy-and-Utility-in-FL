{
    "dataset": "mnist",
    "mode": "classic",
    "task": "fairness",
    "debug": true,
    "save_model": false,
    "wandb": true,
    "data_split_config": {
        "split_type": "stratified",
        "max_samples_per_cluster": [1000, 2000, 3000, 4000, 5000, 6000, 7000, 8000],
        "num_nodes": 1,
        "num_clusters": 8,
        "noniid_nodes_distribution": false,
        "server_validation_set": "server_validation_split"
    },
    "p2p_config": {
        "differential_privacy_pre_training_cluster": false,
        "differential_privacy_mixed_mode_cluster": false,
        "local_training_epochs_in_cluster": 1,
        "num_communication_round_pre_training": [
            0
        ],
        "num_communication_round_mixed_mode": 1
    },
    "server_config": {
        "differential_privacy_server": false,
        "mixed_mode": false,
        "local_epochs": 1,
        "fl_rounds": 30,
        "total_mixed_iterations": 1
    },
    "hyperparameters": {
        "batch_size": 128,
        "lr": 0.001,
        "MAX_PHYSICAL_BATCH_SIZE": 128,
        "DELTA": 1e-5,
        "noise_multiplier": 0.8,
        "noise_multiplier_P2P": [
            2.0
        ],
        "max_grad_norm": 20.0,
        "weight_decay": 0,
        "min_improvement": 0.001,
        "patience": 10,
        "min_accuracy": 0.80
    }
}